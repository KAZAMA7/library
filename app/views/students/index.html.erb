<h1>Listing students</h1>
 
<table>
  <tr>
    <th>First_Name</th>
    <th>Last_Name</th>
    <th>Date_Of_Birth</th>
    <th>Address</th>
    <th>Phone</th>
  </tr>
 
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.date_of_birth %></td>
      <td><%= student.address %></td>
      <td><%= student.phone %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
    </tr>
  <% end %>
</table>