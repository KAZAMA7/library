<%= form_with scope: :student, url: students_path, local: true do |form| %>
  <p>
    <%= form.label :first_name %><br>
    <%= form.text_field :string %>
  </p>
 
  <p>
    <%= form.label :last_name %><br>
    <%= form.text_area :string %>
  </p>
 
 <p>
    <%= form.label :date_of_birth %><br>
    <%= form.text_area :date %>
  </p>
 
 <p>
    <%= form.label :phone %><br>
    <%= form.text_area :string %>
  </p>
 
 <p>
    <%= form.label :address %><br>
    <%= form.text_area :text %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>